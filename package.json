{
  "name": "@dgrishajev/react-mui-kit",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/dgrishajev"
  },
  "version": "1.4.12",
  "description": "React Material UI Kit",
  "license": "ISC",
  "author": "Dominic Grishajev",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:module && yarn build:main && tsc",
    "build:module": "babel src --out-dir dist --extensions '.ts' --presets @babel/preset-typescript --plugins @babel/plugin-transform-runtime",
    "build:main": "babel src --out-dir dist/esm --extensions '.ts' --presets @babel/preset-env --plugins @babel/plugin-transform-runtime"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^17.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.66",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "tss-react": "^4.0.0-beta.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
